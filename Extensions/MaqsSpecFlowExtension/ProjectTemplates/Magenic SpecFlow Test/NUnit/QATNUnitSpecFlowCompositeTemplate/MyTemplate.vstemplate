<VSTemplate Version="3.0.0" Type="ProjectGroup"
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
        <Name>MAQS SpecFlow Composite Tests - NUnit</Name>
        <DefaultName>CompositeTests</DefaultName>
        <Description>Magenic's automation quick start framework for composite SpecFlow testing</Description>
        <Icon>QATTest.ico</Icon>
        <ProjectType>CSharp</ProjectType>
        <LanguageTag>C#</LanguageTag>
        <PlatformTag>windows</PlatformTag>
        <ProjectTypeTag>test</ProjectTypeTag>
        <ProjectTypeTag>Magenic SpecFlow Test</ProjectTypeTag>
        <ProjectSubType>Magenic SpecFlow Test</ProjectSubType>
    </TemplateData>
    <TemplateContent>
        <ProjectCollection>
                <ProjectTemplateLink ProjectName="Models">
                    Models\MyTemplate.vstemplate
                </ProjectTemplateLink>
                <ProjectTemplateLink ProjectName="Tests">
                    Tests\MyTemplate.vstemplate
                </ProjectTemplateLink>
        </ProjectCollection>
    </TemplateContent>
</VSTemplate>